<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"      
 "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
 <mapper namespace="com.ssm.dao.MyWatchDao">
 <select id="myWatch" parameterType="map" resultType="com.ssm.entity.WatchInf">
 		SELECT 
 			*
		FROM
			t_watch
		WHERE
			loginusername = #{0}
</select>
 </mapper>